{
  "name": "root",
  "private": true,
  "scripts": {
    "build": "turbo run build ",
    "build:dev": "turbo run build:dev ",
    "lint:root": "eslint --fix --resolve-plugins-relative-to ./ --ext .ts,.tsx,.jsx,.json,.vue ./resources/[dg]/",
    "lint": "pnpm run lint:root --quiet",
    "format": "turbo run format",
    "format:staged": "pretty-quick --staged --pattern '**/*.{ts,tsx,scss,css,md,vue,json}' && lint-staged",
    "db:push": "turbo run db:push",
    "db:deploy": "turbo run db:deploy",
    "prepare": "husky install"
  },
  "dependencies": {
    "@anatine/esbuild-decorators": "^0.2.19",
    "@citizenfx/client": "2.0.6471-1",
    "@citizenfx/server": "2.0.6471-1",
    "@jgoz/esbuild-plugin-typecheck": "^3.0.2",
    "dayjs": "^1.11.7",
    "esbuild": "^0.17.19",
    "esbuild-loader": "^3.0.0",
    "esbuild-plugin-clean": "^1.0.1",
    "esbuild-plugin-fileloc": "^0.0.6",
    "esbuild-plugin-import-pattern": "^0.1.0",
    "javascript-obfuscator": "^4.0.2",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@sentry/esbuild-plugin": "^2.2.0",
    "@sentry/integrations": "^7.53.1",
    "@sentry/node": "^7.53.1",
    "@sentry/tracing": "^7.53.1",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "turbo": "^1.9.9"
  },
  "lint-staged": {
    "*.{ts,tsx,vue,json}": "eslint --fix --resolve-plugins-relative-to ./ --ext .ts,.tsx,.json,.vue ./resources/[dg]/ --cache"
  },
  "workspaces": [
    "resources/**",
    "packages/*"
  ],
  "prisma": {
    "seed": "ts-node -P packages/db/tsconfig.json packages/db/seed.ts"
  },
  "version": "1.3.1",
  "pnpm": {
    "patchedDependencies": {
      "vite-plugin-sentry@1.2.0": "patches/vite-plugin-sentry@1.2.0.patch"
    }
  }
}