{
  "name": "dg-ui",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "vite --mode development",
    "viteMem": "node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js",
    "build": "tsc && yarn viteMem build --mode production",
    "build:dev": "tsc && yarn viteMem build --mode gamedev",
    "watch": "vite build --mode gamedev --watch",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --ignore-path ./.prettierignore ",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.eslintignore -c ./.eslintrc"
  },
  "dependencies": {
    "@degrens-21/fa-6": "^1.0.1",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "@mui/styles": "^5.10.2",
    "@remirror/pm": "^1.0.18",
    "@remirror/react": "^1.0.36",
    "@remirror/styles": "^1.1.3",
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "dayjs": "^1.11.5",
    "deepmerge": "^4.2.2",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dnd": "^16.0.1",
    "react-dnd-preview": "^6.0.2",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-number-format": "^4.9.3",
    "react-redux": "^8.0.2",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.13.4",
    "react-spring": "^9.5.2",
    "react-use-measure": "^2.1.1",
    "redux": "^4.2.0",
    "remirror": "^1.0.79",
    "sass": "^1.54.5",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@surma/rollup-plugin-off-main-thread": "^2.2.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "typescript": "^4.7.4",
    "vite": "3.0.9",
    "vite-plugin-checker": "^0.5.0",
