{
  "name": "dg-ui",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "vite --mode development",
    "build": "tsc --noEmit && vite build --mode production",
    "build:debug": "tsc --noEmit && vite build --mode gamedev",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --ignore-path ./.prettierignore ",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.eslintignore -c ./.eslintrc",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.2",
    "@mui/styles": "^5.5.1",
    "@remirror/pm": "^1.0.14",
    "@remirror/react": "^1.0.28",
    "@remirror/styles": "^1.1.3",
    "@sentry/react": "^6.19.1",
    "@sentry/tracing": "^6.19.1",
    "dayjs": "^1.11.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "^17.0.2",
    "react-colorful": "^5.5.1",
    "react-dom": "^17.0.2",
    "react-number-format": "^4.9.1",
    "react-redux": "^7.2.6",
    "react-scripts": "^5.0.0",
    "react-spring": "^9.4.4",
    "redux": "^4.1.2",
    "remirror": "^1.0.71",
    "sass": "^1.49.9",
    "urijs": "^1.19.10"
  },
  "devDependencies": {
