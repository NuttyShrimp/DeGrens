{
  "name": "dg-ui",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "vite --mode development",
    "viteMem": "node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js",
    "build": "tsc && yarn viteMem build --mode production",
    "build:dev": "tsc && yarn viteMem build --mode gamedev",
    "watch": "vite build --mode gamedev --watch",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --ignore-path ./.prettierignore ",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.eslintignore -c ./.eslintrc",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@degrens-21/fa-6": "^1.0.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.4",
    "@mui/styles": "^5.6.2",
    "@remirror/pm": "^1.0.18",
    "@remirror/react": "^1.0.36",
    "@remirror/styles": "^1.1.3",
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "dayjs": "^1.11.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "^18.1.0",
    "react-colorful": "^5.5.1",
    "react-dom": "^18.1.0",
    "react-number-format": "^4.9.3",
    "react-redux": "^8.0.1",
    "react-scripts": "^5.0.1",
    "react-spinners": "^0.11.0",
    "react-spring": "^9.4.5",
    "react-use-measure": "^2.1.1",
    "redux": "^4.2.0",
    "remirror": "^1.0.79",
    "sass": "^1.51.0",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@surma/rollup-plugin-off-main-thread": "^2.2.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vite-plugin-checker": "^0.4.6",
    "vite-plugin-sentry": "^1.0.14",
    "vite-plugin-svgr": "^2.1.0",
    "vite-tsconfig-paths": "^3.4.1"
  }
}
